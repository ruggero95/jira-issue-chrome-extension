{
  "short_name": "Jira Backlog Extension",
  "name": "Jira Backlog Extension",
  "description":"fast copy of jira commit code and issue list with filters",
  "version": "2.2",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "declarativeNetRequestFeedback" 
  ],
  "host_permissions": [
    "https://*.atlassian.net/rest/agile/*",
    "https://*.atlassian.net/rest/api/*"
  ],
  "start_url": ".",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#ffffff"
}